    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.8",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
