  "version": "1.0.0",
  "description": "Convert banka frontend to react app",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --mode development --config ./config/webpack.config.js --open --history-api-fallback",
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
